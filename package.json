{
	"name": "quebec-health-data",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev --env local",
		"start": "wrangler dev --env local",
		"test": "vitest",
		"cf-typegen": "wrangler types",
		"create-local-db": "wrangler d1 execute health-db --local --file=./schema.sql",
		"create-remote-db": "wrangler d1 execute health-db --remote --file=./schema.sql",
		"create-sql-schema": "node src/scripts/autogenSchema.mts",
		"create-openapi-schema": "chanfana -o ./schema.json --env local",
		"fetch-regions": "node src/data/extraction/fetchRegions.mts",
		"fetch-births": "node src/data/extraction/fetchBirths.mts",
		"fetch-data": "node src/data/extraction/fetchWaitlistData.mts"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"@types/node": "^25.0.3",
		"typescript": "^5.5.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.54.0"
	},
	"type": "module",
	"dependencies": {
		"axios": "^1.13.2",
		"chanfana": "^3.0.0",
		"itty-fetcher": "^1.0.10",
		"itty-router": "^5.0.22",
		"shpjs": "^6.2.0",
		"zod": "^4.3.5"
	}
}
